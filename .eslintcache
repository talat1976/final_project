   השונים כולל ציוד נלווה , חלקי חומרה למחשבים .\r\n                        ואנחנו מטפלים בכל מיני בעיות\r\n                        , (אינטרנט , כל סוגי תקלות המחשבים, והתוכנה , והסמרטפונים השונים)\r\n                        מתמחים גם בבניית אתרים כולל עיצוב .\r\n                        עקב מצב הקרונה במדינה ומתוך דאגה לכלל לקוחותינו החלטנו\r\n                        להעלות את החנות לאתר ,על מנת להקל עליכם הדבר שמאפשר\r\n                        רכישות און לין דרך האתר כולל משלוח עד הבית .\r\n                        האתר נותן גם שירותי תמיכה טכנית רק ללקוחות\r\n                        שנרשמו כחברים ,ואנחנו מציעים לכלל לקוחותינו להירשם\r\n                        .זה חינמי מטעם החנות\r\n                        האתר מאפשר ללקוח חווית רכישה נוחה ומתקדמת של .\r\n                        באתר מגוון כלים חכמים המסייעים ללקוח לקבל החלטות צרכניות נבונות מיטביות, ללא צורך בידע קודם.\r\n\r\n                        מגוון עצום של פריטים הנחוצים למחשבים ולפלאפונים, מחשבים וטכנולוגיה מתקדמת.\r\n                        לקוחתנו נהנים ממערך הפצה עד בית הלקוח תוך הקפדה על איכות הפצה גבוהה ומדויקת,\r\n                        אריזה איכותית והקפדה על מועד אספקה קצר במיוחד,\r\n                        בדרך כלל 14 ימי עסקים.\r\n\r\n                        הסביבה משתנה במהירות מתמדת ולכן חדשנות היא גורם קריטי לכושר התחרות שלנו במיוחד לאחר המצב של הקורונה.\r\n                        בקטלוג ובאתר האינטרנט הם מוצרים מובחרים שנרכשו מספקים אמינים, עם תווית איכות גבוהה ובמחיר תחרותי.\r\n\r\n                        חזון וערכים : החזון שלנו הוא להנהיג את שוק הציוד הטכנולוגי לפעול להעצמת הלקוח באופן שיוכל\r\n                        לקבל החלטות צרכניות מיטביות וליישמן ללא חסמי ידע ומומחיות.\r\n                        נמשיך להציע את תנאי הקניה הטובים ביותר, עם שירות והדרכה בסטנדרטים הגבוהים ביותר.\r\n\r\n\r\n\r\n                        <h5> .ואנחנו מציעים לכלל לקוחותינו להירשם</h5>\r\n\r\n\r\n\r\n\r\n\r\n                    </p>\r\n\r\n\r\n\r\n                </div>\r\n\r\n            </div>\r\n        </div>\r\n    )\r\n}\r\n",["114","115"],"C:\\Users\\talat\\Desktop\\project\\src\\pages\\product\\Product.js",["116"],"import React, { useEffect, useState } from 'react'\r\nimport { useCookies } from 'react-cookie'\r\nimport Rating from '../../components/rating/Rating'\r\nimport { firebaseDB } from '../../services/firebase'\r\nimport \"./product.css\"\r\n\r\n\r\nconst Product = (props) => {\r\n    const [product, setProduct] = useState()\r\n    const [cookies, setCookie] = useCookies(['cart'])\r\n    const productId = props.match.params.id\r\n\r\n    useEffect(() => {\r\n        const fetch = async () => {\r\n            const prodRef = firebaseDB.collection(\"products\").doc(productId)\r\n            const doc = await prodRef.get()\r\n            setProduct(doc.data())\r\n        }\r\n\r\n        fetch()\r\n    }, [productId])\r\n\r\n    const addToCart = () => {\r\n        if (cookies.cart) {\r\n            setCookie(\"cart\", `${cookies.cart},${productId}`)\r\n        } else {\r\n            setCookie(\"cart\", `${productId}`)\r\n        }\r\n    }\r\n\r\n    if (!product) {\r\n        return <div>Loading...</div>\r\n    }\r\n\r\n    return (\r\n        <div className=\"main-content\">\r\n            <div className=\"card\">\r\n                <div className=\"image\">\r\n                    <img src={product.image} />\r\n                </div>\r\n                <h4>{product.name}</h4>\r\n                <p>{product.desc}</p>\r\n                <h2>₪{product.price}</h2>\r\n                {product.warranty ? <div>אחריות: {product.warranty}</div> : null}\r\n                {product.comp && <div>מותג: {product.comp}</div>}\r\n                {product.rating > 0 && <Rating rating={product.rating} />}\r\n\r\n                <div className=\"mt-4\">\r\n                    {product.qty > 0 ?\r\n                        <button className=\"btn btn-success\" onClick={addToCart}>הוספה לסל</button>\r\n                        :\r\n                        <div className=\"not_found\">מוצר אזל מהמלאי</div>\r\n\r\n                    }\r\n                </div>\r\n            </div>\r\n        </div>\r\n\r\n\r\n    )\r\n}\r\n\r\nexport default Product\r\n",["117","118"],"C:\\Users\\talat\\Desktop\\project\\src\\App.js",[],["119","120"],"C:\\Users\\talat\\Desktop\\project\\src\\components\\header\\Header.js",["121"],"C:\\Users\\talat\\Desktop\\project\\src\\pages\\reports\\Reports.js",[],["122","123"],"C:\\Users\\talat\\Desktop\\project\\src\\pages\\admin\\Admin.js",[],"C:\\Users\\talat\\Desktop\\project\\src\\pages\\admin\\reports\\ReportsList.js",["124","125","126","127"],"C:\\Users\\talat\\Desktop\\project\\src\\pages\\Errors\\E404.js",["128","129"],"C:\\Users\\talat\\Desktop\\project\\src\\pages\\admin\\AdminLogin.js",[],"C:\\Users\\talat\\Desktop\\project\\src\\pages\\admin\\products\\ProductModal.js",[],"C:\\Users\\talat\\Desktop\\project\\src\\pages\\admin\\reports\\ReportItem.js",["130","131","132","133","134"],"C:\\Users\\talat\\Desktop\\project\\src\\pages\\home\\Home.js",["135","136"],"import React, { useEffect, useState } from 'react'\r\nimport \"./home.css\"\r\nimport ProductItem from \"../../components/product/ProductItem\"\r\nimport { firebaseDB } from '../../services/firebase'\r\nimport { Link } from 'react-router-dom'\r\nimport { useCookies } from 'react-cookie'\r\n\r\nconst Home = (props) => {\r\n\r\n    const [products, setProducts] = useState([])\r\n    const [filterdProducts, setFilterdProducts] = useState([])\r\n    const [filter, setFilter] = useState(\"\")\r\n    const [search, setSearch] = useState(\"\")\r\n    const [categories, setCategories] = useState([])\r\n\r\n    const [cookies, setCookie] = useCookies(['cart'])\r\n\r\n    useEffect(() => {\r\n        const unsub = firebaseDB.collection(\"products\").onSnapshot((snap) => {\r\n            const arr = snap.docs.map(item => ({ id: item.id, ...item.data() }))\r\n            setProducts(arr)\r\n            setFilterdProducts(arr)\r\n        },\r\n            (error) => console.log(error),\r\n            () => console.log(\"complete\")\r\n        )\r\n\r\n        firebaseDB.collection(\"categories\").onSnapshot((snap) => {\r\n            const arr = snap.docs.map(item => ({ id: item.id, ...item.data() }))\r\n            setCategories(arr)\r\n        },\r\n            (error) => console.log(error),\r\n            () => console.log(\"complete\")\r\n        )\r\n\r\n        return () => unsub()\r\n    }, [])\r\n\r\n    useEffect(() => {\r\n        let searchProduct = []\r\n\r\n        if (filter) {\r\n            if (filter === \"sale\") {\r\n                const prods = products.filter(p => p.sale > 0)\r\n                searchProduct = prods\r\n            } else {\r\n                const prods = products.filter(p => p.category === filter)\r\n                searchProduct = prods\r\n            }\r\n        } else {\r\n            searchProduct = products\r\n        }\r\n\r\n        if (search.length > 0) {\r\n            searchProduct = searchProduct.filter(p => p.name.toLowerCase().includes(search.toLowerCase()))\r\n        }\r\n\r\n        setFilterdProducts(searchProduct)\r\n    }, [filter, search])\r\n\r\n    const onView = (id) => {\r\n        props.history.push(`/product/${id}`)\r\n\r\n    }\r\n\r\n    const onAddToCart = (id) => {\r\n        if (cookies.cart) {\r\n            setCookie(\"cart\", `${cookies.cart},${id}`)\r\n        } else {\r\n            setCookie(\"cart\", `${id}`)\r\n        }\r\n    }\r\n\r\n    return (\r\n        <div className=\"page\" id=\"home\">\r\n            <div className=\"links\">\r\n                <div\r\n                    className={`link ${filter === \"\" ? \"active\" : \"\"}`}\r\n                    onClick={() => setFilter(\"\")}\r\n                >\r\n                    הכל\r\n                </div>\r\n\r\n                <div className={`link ${filter === \"sale\" ? \"active\" : \"\"}`} onClick={() => setFilter(\"sale\")}>מבצע השבוע</div>\r\n\r\n                {categories.map(category =>\r\n                    <div\r\n                        key={category.id}\r\n                        className={`link ${filter === category.value ? \"active\" : \"\"}`}\r\n                        onClick={() => setFilter(category.value)}\r\n                    >\r\n                        {category.display}\r\n                    </div>\r\n                )}\r\n\r\n            </div>\r\n\r\n            {(filter === \"sale\") && <h1 className=\"sale-banner\"> TALAT TECH  מבצעי השבוע  ב  </h1>}\r\n\r\n            <div className=\"search\">\r\n                <label>חיפוש</label>\r\n                <input value={search} onChange={(e) => setSearch(e.target.value)} placeholder=\"חיפוש\" />\r\n            </div>\r\n\r\n            <div className=\"products\">\r\n                {filterdProducts.map(prod =>\r\n                    <ProductItem\r\n                        key={prod.id}\r\n                        onAddToCart={onAddToCart}\r\n                        product={prod}\r\n                        onClick={() => onView(prod.id)}\r\n                    />\r\n                )}\r\n            </div>\r\n        </div>\r\n    )\r\n\r\n}\r\n\r\nexport default Home\r\n","C:\\Users\\talat\\Desktop\\project\\src\\pages\\contact\\Contact.js",["137"],"import React from 'react'\r\nimport './contact.css'\r\nimport p5 from \"../../images/TalatTech.png\"\r\n\r\nexport default function Contact() {\r\n  return (\r\n\r\n    <div className=\"contact\">\r\n\r\n      <h1><u>אנו עומדים לשירותכם</u></h1>\r\n\r\n      <div className=\"abut\">\r\n        <h3> בימים א'-ה' בין השעות 18:30 - 12:00 </h3>\r\n        <h3>כתובת: עין נקובא, רחוב הגנים 114   </h3>\r\n        <h3> talat.salmh21@gmail.com :דוא\"ל</h3>\r\n        <h3> טלפון: 052-3034046 </h3>\r\n        <h3> טלפקס: 02-5709321 </h3>\r\n      </div>\r\n\r\n      <img src={p5} />\r\n\r\n    </div>\r\n  )\r\n}\r\n","C:\\Users\\talat\\Desktop\\project\\src\\components\\product\\ProductItem.js",["138"],"C:\\Users\\talat\\Deskto
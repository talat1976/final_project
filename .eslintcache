 }\r\n\r\n\r\n    return (\r\n        <Container>\r\n            <h1>  סל הקניות </h1>\r\n\r\n            {loading && <Spinner animation=\"border\" />}\r\n\r\n            {products.map(prod =>\r\n                <div className=\"cart-product\" key={prod.id}>\r\n                    <div className=\"cart-prodcut-info\">\r\n                        <img src={prod.image} />\r\n                        <h3>{prod.name}</h3>\r\n                    </div>\r\n                    <div className=\"cart-prodcut-info\">\r\n                        <h4>₪{prod.price}</h4>\r\n                        <Button size=\"sm\" variant=\"danger\" onClick={() => onDeleteItem(prod.id)}>\r\n                            <FontAwesomeIcon icon={faTrash} />\r\n                        </Button>\r\n                    </div>\r\n                </div>\r\n            )}\r\n\r\n            {cookies.cart ?\r\n\r\n                <div style={{ textAlign: \"right\" }}>\r\n\r\n                    <h4> סה\"כ לתשלום: ₪{total}</h4>\r\n                    <Button onClick={onPayment}>לתשלום</Button>\r\n                </div>\r\n                :\r\n                <div className=\"cart-empty\">הסל ריק </div>\r\n            }\r\n\r\n        </Container>\r\n    )\r\n}\r\n\r\nexport default Cart\r\n","C:\\Users\\talat\\Desktop\\project\\src\\pages\\payment\\Payment.js",[],"C:\\Users\\talat\\Desktop\\project\\src\\pages\\admin\\products\\ProductsList.js",["143","144"],"C:\\Users\\talat\\Desktop\\project\\src\\pages\\admin\\orders\\Orders.js",[],["145","146"],"C:\\Users\\talat\\Desktop\\project\\src\\pages\\admin\\products\\EditProduct.js",[],"C:\\Users\\talat\\Desktop\\project\\src\\pages\\admin\\products\\CreateProducts.js",[],"C:\\Users\\talat\\Desktop\\project\\src\\pages\\admin\\orders\\OrderProduct.js",["147","148"],"C:\\Users\\talat\\Desktop\\project\\src\\pages\\admin\\categories\\Categories.js",[],"C:\\Users\\talat\\Desktop\\project\\src\\components\\rating\\Rating.js",[],{"ruleId":"149","severity":1,"message":
              <div class=\"profile\">\r\n\r\n                    <div className=\"img-cont mt-5\">\r\n                        <img className=\"talat_foto\" src={p4} />\r\n                    </div>\r\n                </div>\r\n\r\n                <div class=\"info\">\r\n\r\n                    <h1><u>קצת עלינו</u></h1>\r\n                    <h3> talat.salmh21@gmail.com :דוא\"ל</h3>\r\n                    <h3>כתובת: עין נקובא, רחוב הגנים 114 │ טלפון: 0523034046 </h3>\r\n\r\n                    <p className=\"para\">\r\n                                                סיום לימודי הנדסאי תוכנה בהתמחות סייבר באוניברסיטת אריאל .\r\n                            החנות כבר קיימת משנת 2010 .\r\n                            אנחנו מוכרים כל סוגי המחשבים והסמרטפונים\r\n                            השונים כולל ציוד נלווה , חלקי חומרה למחשבים .\r\n                            ואנחנו מטפלים בכל מיני בעיות\r\n                            , (אינטרנט , כל סוגי תקלות המחשבים, והתוכנה , והסמרטפונים השונים) \r\n                            מתמחים גם בבניית אתרים כולל עיצוב .\r\n                            עקב מצב הקרונה במדינה ומתוך דאגה לכלל לקוחותינו החלטנו \r\n                            להעלות את החנות לאתר ,על מנת להכל עליכם הדבר שמאפשר \r\n                            רכישות און לין דרך האתר כולל משלוח עד הבית .\r\n                            האתר נותן גם שירותי תמיכה טכנית רק ללקוחות \r\n                            שנרשמו כחברים ,ואנחנו מציעים לכלל לקוחותינו להירשם \r\n                            .זה חינמי מטעם החנות \r\n                            האתר מאפשר ללקוח חווית רכישה נוחה ומתקדמת של .\r\n                            באתר מגוון כלים חכמים המסייעים ללקוח לקבל החלטות צרכניות נבונות מיטביות, ללא צורך בידע קודם.\r\n                            \r\n                            מגוון עצום של פריטים הנחוצים למחשבים ולפלאפונים, מחשבים וטכנולוגיה מתקדמת.\r\n                            לקוחתנו נהנים ממערך הפצה עד בית הלקוח תוך הקפדה על איכות הפצה גבוהה ומדויקת,\r\n                            אריזה איכותית והקפדה על מועד אספקה קצר במיוחד,\r\n                            בדרך כלל 14 ימי עסקים.\r\n                            \r\n                            הסביבה משתנה במהירות מתמדת ולכן חדשנות היא גורם קריטי לכושר התחרות שלנו במיוחד לאחר המצב של הקורונה.\r\n                            בקטלוג ובאתר האינטרנט הם מוצרים מובחרים שנרכשו מספקים אמינים, עם תווית איכות גבוהה ובמחיר תחרותי.\r\n                            \r\n                            חזון וערכים : החזון שלנו הוא להנהיג את שוק הציוד הטכנולוגי לפעול להעצמת הלקוח באופן שיוכל \r\n                            לקבל החלטות צרכניות מיטביות וליישמן ללא חסמי ידע ומומחיות.\r\n                            נמשיך להציע את תנאי הקניה הטובים ביותר, עם שירות והדרכה בסטנדרטים הגבוהים ביותר. \r\n\r\n\r\n                        \r\n                        <h5> .ואנחנו מציעים לכלל לקוחותינו להירשם</h5>\r\n\r\n\r\n\r\n\r\n\r\n                    </p>\r\n\r\n\r\n\r\n                </div>\r\n\r\n            </div>\r\n        </div>\r\n    )\r\n}\r\n",["124","125"],"C:\\Users\\talat\\Desktop\\student talat\\ACADEMI_TOW_2\\WORKSPACE\\react\\project\\src\\pages\\Errors\\E404.js",[],["126","127"],"C:\\Users\\talat\\Desktop\\student talat\\ACADEMI_TOW_2\\WORKSPACE\\react\\project\\src\\pages\\login\\Login.js",[],"C:\\Users\\talat\\Desktop\\student talat\\ACADEMI_TOW_2\\WORKSPACE\\react\\project\\src\\pages\\contact\\Contact.js",[],"C:\\Users\\talat\\Desktop\\student talat\\ACADEMI_TOW_2\\WORKSPACE\\react\\project\\src\\services\\firebase.js",[],"C:\\Users\\talat\\Desktop\\student talat\\ACADEMI_TOW_2\\WORKSPACE\\react\\project\\src\\pages\\admin\\Admin.js",["128"],"C:\\Users\\talat\\Desktop\\student talat\\ACADEMI_TOW_2\\WORKSPACE\\react\\project\\src\\pages\\admin\\AdminLogin.js",[],"C:\\Users\\talat\\Desktop\\student talat\\ACADEMI_TOW_2\\WORKSPACE\\react\\project\\src\\pages\\admin\\products\\CreateProducts.js",["129"],"import React, { useState } from 'react'\r\nimport { Modal } from 'react-bootstrap'\r\nimport ReactModal from 'react-modal'\r\nimport { firebaseDB, firebaseStorage, timestamp } from '../../../services/firebase'\r\n\r\nexport default function CreateProducts(props) {\r\n\r\n    const [product, setProduct] = useState({\r\n        name: \"\",\r\n        price: 0,\r\n        qty: 0,\r\n        image: undefined,\r\n        desc: \"\",\r\n        sale: 0,\r\n        comp: \"\"\r\n    })\r\n\r\n    const onChange = (e) => {\r\n        setProduct({ ...product, [e.target.name]: e.target.value })\r\n    }\r\n\r\n    const onFileChange = (e) => {\r\n        setProduct({ ...product, image: e.target.files[0] })\r\n    }\r\n\r\n    const onAdd = async () => {\r\n        const file = product.image\r\n\r\n        if (file) {\r\n            const uploadTask = firebaseStorage.child(file.name).put(file)\r\n\r\n            uploadTask.on(\"state_changed\", (snap) => {\r\n                console.log(snap)\r\n            }, (error) => console.log(error),\r\n                () => {\r\n                    uploadTask.snapshot.ref.getDownloadURL().then((downloadURL) => {\r\n                        console.log('File available at', downloadURL);\r\n\r\n                        firebaseDB.collection(\"products\").add({\r\n                            name: product.name,\r\n                            price: +product.price,\r\n                            qty: +product.qty,\r\n                            image: downloadURL,\r\n                            desc: product.desc,\r\n                            sale: +product.sale,\r\n                            comp: product.comp,\r\n                            created: timestamp()\r\n                        })\r\n                            .then(docRef => props.onSuccess())\r\n                            .catch((err) => console.log(err))\r\n                    })\r\n                }\r\n            )\r\n        }\r\n    }\r\n\r\n    return (\r\n        <Modal show={props.openmodal} onHide={props.onClose}>\r\n            <Modal.Header closeButton>\r\n                <Modal.Title>Add new product</Modal.Title>\r\n            </Modal.Header>\r\n            <div className=\"p-4\">\r\n                <div>\r\n                    <label>Name</label>\r\n                    <input className=\"form-control\" type=\"text\" name=\"name\" value={product.name} onChange={onChange} />\r\n                </div>\r\n\r\n                <div>\r\n                    <label>Price</label>\r\n                    <input className=\"form-control\" type=\"number\" name=\"price\" value={product.price} onChange={onChange} />\r\n                </div>\r\n\r\n                <div>\r\n                    <label>Quantity</label>\r\n                    <input className=\"form-control\" type=\"number\" name=\"qty\" value={product.qty} onChange={onChange} />\r\n                </div>\r\n\r\n                <div>\r\n                    <label>Image</label>\r\n                    <input className=\"form-control\" type=\"file\" onChange={onFileChange} />\r\n                </div>\r\n\r\n                <div>\r\n                    <label>Description</label>\r\n                    <input className=\"form-control\" type=\"text\" name=\"desc\" value={product.desc} onChange={onChange} />\r\n                </div>\r\n\r\n                <div>\r\n                    <label>Sale</label>\r\n                    <input className=\"form-control\" type=\"number\" name=\"sale\" value={product.sale} onChange={onChange} />\r\n                </div>\r\n\r\n                <div>\r\n                    <label>Company</label>\r\n                    <input className=\"form-control\" type=\"text\" name=\"comp\" value={product.company} onChange={onChange} />\r\n                </div>\r\n\r\n                <button className=\"btn btn-primary mt-3\" onClick={onAdd}>Add</button>\r\n            </div>\r\n        </Modal>\r\n    )\r\n}\r\n","C:\\Users\\talat\\Desktop\\student talat\\ACADEMI_TOW_2\\WORKSPACE\\react\\project\\src\\pages\\admin\\products\\ProductsList.js",["130"],"im